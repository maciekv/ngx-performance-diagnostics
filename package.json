{
  "name": "ngx-performance-diagnostics",
  "version": "1.0.2",
  "description": "Angular library for detecting performance issues, change detection cycles, and memory leaks",
  "keywords": [
    "angular",
    "performance",
    "diagnostics",
    "change-detection",
    "memory-leak",
    "profiling",
    "debugging",
    "devtools"
  ],
  "author": "Maciej Osytek <maciej.osytek@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/maciekv/ngx-performance-diagnostics.git"
  },
  "bugs": {
    "url": "https://github.com/maciekv/ngx-performance-diagnostics/issues"
  },
  "homepage": "https://github.com/maciekv/ngx-performance-diagnostics#readme",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "esm2022": "./dist/esm2022/index.mjs",
      "es2020": "./dist/esm2020/index.mjs",
      "default": "./dist/index.mjs"
    }
  },
  "scripts": {
    "build": "ng-packagr -p ng-package.json",
    "pack": "npm run build && cd dist && npm pack",
    "publish:npm": "npm run build && cd dist && npm publish",
    "test": "echo \"Tests coming soon\" && exit 0",
    "lint": "eslint src/**/*.ts"
  },
  "peerDependencies": {
    "@angular/common": "^17.0.0 || ^18.0.0 || ^19.0.0",
    "@angular/core": "^17.0.0 || ^18.0.0 || ^19.0.0",
    "rxjs": "^7.0.0"
  },
  "devDependencies": {
    "@angular/common": "^18.0.0",
    "@angular/core": "^18.0.0",
    "@angular/compiler": "^18.0.0",
    "@angular/compiler-cli": "^18.0.0",
    "ng-packagr": "^18.0.0",
    "typescript": "~5.4.0",
    "rxjs": "^7.8.0",
    "tslib": "^2.6.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
